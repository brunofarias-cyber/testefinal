{
  "name": "bprojetos-backend",
  "version": "1.0.0",
  "description": "Backend API para plataforma educacional BProjetos",
  "main": "server.js",
  "type": "module",
  "overrides": {
    "debug": "4.3.4"
  },
  "scripts": {
    "start": "node server.js",
    "start:backend": "node server.js",
    "start:frontend": "vite",
    "server": "node server.js",
    "server:dev": "nodemon server.js",
    "client": "vite",
    "dev": "concurrently \"npm run server:dev\" \"npm run client\"",
    "dev:simple": "concurrently \"node server-dev.js\" \"npm run client\"",
    "dev:server-only": "npm run server",
    "build": "vite build",
    "build:render": "vite build",
    "preview": "vite preview",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:bncc": "NODE_OPTIONS=--experimental-vm-modules jest tests/bncc.test.js",
    "test:components": "NODE_OPTIONS=--experimental-vm-modules jest src/components/__tests__",
    "seed": "node scripts/seed-indicators.js",
    "seed:bncc": "node seeds/bncc-data.js",
    "seed:coteaching": "node seeds/coteaching-seed.js",
    "setup:coteaching": "node scripts/run-coteaching-migration.js",
    "reset:coteaching": "node scripts/reset-coteaching-tables.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo:all",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,css,md}\"",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:run": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart"
  },
  "keywords": [
    "education",
    "projects",
    "api",
    "nodejs"
  ],
  "author": "BProjetos Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.1",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "googleapis": "^167.0.0",
    "jsonwebtoken": "^9.0.0",
    "lucide-react": "^0.294.0",
    "multer": "^2.0.2",
    "nodemailer": "^6.10.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.10.1",
    "recharts": "^3.5.1",
    "sequelize": "^6.35.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "prettier": "^3.7.4",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.3.5",
    "terser": "^5.44.1",
    "vite": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seu-usuario/bprojetos-backend.git"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
